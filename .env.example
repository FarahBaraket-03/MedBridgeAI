# Copy this file to .env and fill in your actual keys
# cp .env.example .env

# ── Groq Cloud LLM ───────────────────────────────────────────────────────────
GROQ_API_KEY=your_groq_api_key_here
GROQ_MODEL=openai/gpt-oss-120b

# ── Qdrant Cloud Vector DB ───────────────────────────────────────────────────
QDRANT_CLOUD_URL=your_qdrant_cloud_url_here
QDRANT_API_KEY=your_qdrant_api_key_here

# ── Databricks MLflow Tracking ───────────────────────────────────────────────
DATABRICKS_TOKEN=your_databricks_token_here
DATABRICKS_HOST=https://your-workspace.cloud.databricks.com
MLFLOW_TRACKING_URI=databricks
MLFLOW_REGISTRY_URI=databricks-uc
MLFLOW_EXPERIMENT_ID=your_experiment_id_here

# ── Databricks Model Serving (optional) ──────────────────────────────────────
# DATABRICKS_SERVING_ENDPOINT=medbridge-vector-search
# VECTOR_SEARCH_BACKEND=databricks
